i=0
emojis="游땑游때游댠游땧游땑游때游댠游땧游땙游땙游댠游땙游댠"
arralreadycounted=[]
while i < len(emojis):
    curr=emojis[i]
    if curr not in arralreadycounted:
        arralreadycounted.append(curr)
        counter=0
        for s in emojis:
            if s==curr: 
                counter+=1
        print(f"emoji {curr} 햫햟햧햢햣햫  혜혝쮏쥃 {counter} 햟향")        
    i+=1 

smiled_str = "游땑游때游댠游땧游땑游때游댠游땧游땙游땙游댠游땙游댠"
smile_dict = {}

for smile in smiled_str:
    if smile not in smile_dict:
        smile_dict[smile] = 1
    else:
        smile_dict[smile] += 1

print('Number of each emoji dict:')
for key, value in smile_dict.items():
    print(f'{key} - {value}')